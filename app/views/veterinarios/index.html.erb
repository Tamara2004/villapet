<h1>Listado de Veterinarios</h1>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Cargo</th>
      <th>Asistencia</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @veterinarios.each do |veterinario| %>
      <tr>
        <td><%= veterinario.nombre %></td>
        <td><%= veterinario.cargo %></td>
        <td><%= veterinario.asistencia %></td>
        <td><%= link_to 'Show', veterinario %></td>
        <td><%= link_to 'Edit', edit_veterinario_path(veterinario) %></td>
        <td><%= link_to 'Destroy', veterinario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Veterinario', new_veterinario_path %>
